<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="il.ac.hit.model.BudgetPerMonth" table="BUDGETPERMONTH">
		<id name="budgetID" type="int" column="ID" >
			<generator class="increment"/>
		</id>
		<property name="userName">
			<column name="USERNAME"/>
		</property>
		<property name="budget">
			<column name="BUDGET"/>
		</property>
		<property name="month">
			<column name="MONTH"/>
		</property>
		<property name="year">
			<column name="YEAR"/>
		</property>
	</class>
</hibernate-mapping>